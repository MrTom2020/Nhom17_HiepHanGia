<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".trangcanhan"
    android:background="@drawable/tuybiennut3">

    <RelativeLayout
        android:layout_width="381dp"
        android:layout_height="553dp"
        android:background="@drawable/tuybn4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.55">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="334dp"
            android:layout_height="58dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="39dp"
            android:layout_marginTop="51dp"
            android:layout_marginEnd="37dp"
            android:gravity="center"
            android:text="PERSONAL INFORMATION"
            android:textColor="#fcfb00"
            android:textSize="20dp"
            android:textStyle="italic|bold" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="103dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textView7"
            android:layout_alignParentStart="true"
            android:layout_marginStart="33dp"
            android:layout_marginTop="57dp"
            android:text="Name"
            android:textColor="#fcfb00" />

        <EditText
            android:id="@+id/edttencanhan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"
            android:layout_marginStart="36dp"
            android:layout_marginTop="45dp"
            android:layout_toEndOf="@+id/textView8"
            android:background="@drawable/tuybiennut"
            android:ems="10"
            android:hint="........."
            android:textColor="#fcfb00"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="103dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textView8"
            android:layout_alignParentStart="true"
            android:layout_marginStart="33dp"
            android:layout_marginTop="58dp"
            android:text="Password"
            android:textColor="#fcfb00" />

        <EditText
            android:id="@+id/edtmkcanhan"
            android:layout_width="205dp"
            android:layout_height="44dp"
            android:layout_below="@+id/edttencanhan"
            android:layout_marginStart="36dp"
            android:layout_marginTop="35dp"
            android:layout_toEndOf="@+id/textView10"
            android:background="@drawable/tuybiennut"
            android:ems="10"
            android:hint="......."
            android:textColor="#fcfb00"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="103dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textView10"
            android:layout_alignParentStart="true"
            android:layout_marginStart="33dp"
            android:layout_marginTop="59dp"
            android:text="Address"
            android:textColor="#fcfb00" />

        <EditText
            android:id="@+id/edtdiachicanhan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edtmkcanhan"
            android:layout_marginStart="36dp"
            android:layout_marginTop="40dp"
            android:layout_toEndOf="@+id/textView11"
            android:background="@drawable/tuybiennut"
            android:ems="10"
            android:hint="......."
            android:textColor="#fcfb00"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="103dp"
            android:layout_height="23dp"
            android:layout_below="@+id/textView11"
            android:layout_alignParentStart="true"
            android:layout_marginStart="33dp"
            android:layout_marginTop="48dp"
            android:text="Date of birth"
            android:textColor="#fcfb00" />

        <EditText
            android:id="@+id/edtngaysinhcanhan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edtdiachicanhan"
            android:layout_marginStart="36dp"
            android:layout_marginTop="30dp"
            android:layout_toEndOf="@+id/textView12"
            android:background="@drawable/tuybiennut"
            android:ems="10"
            android:textColor="#fcfb00"
            android:inputType="datetime"/>

        <Button
            android:id="@+id/btndycn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edtngaysinhcanhan"
            android:layout_alignParentStart="true"
            android:layout_marginStart="76dp"
            android:layout_marginTop="42dp"
            android:background="@drawable/tuybn4"
            android:text="Log out"
            android:textColor="#fcfb00" />

        <Button
            android:id="@+id/btnluucanhan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/edtngaysinhcanhan"
            android:layout_marginStart="68dp"
            android:layout_marginTop="42dp"
            android:layout_toEndOf="@+id/btndycn"
            android:background="@drawable/tuybn4"
            android:text="Save"
            android:textColor="#fcfb00" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>